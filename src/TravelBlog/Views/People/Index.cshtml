@model IEnumerable<TravelBlog.Models.People>

<html>
<head>
    <title>Travel Blog</title>
</head>
<body>
    <h2>TravelBlog</h2>
    <h4>People!</h4>
    <ul>
        @foreach (var listItem in Model)
        {
            <li>@Html.DisplayFor(modelItem => listItem.people_name) | @Html.ActionLink("Details", "Details", new { id = listItem.id })</li>
        }
    </ul>
</body>
</html>